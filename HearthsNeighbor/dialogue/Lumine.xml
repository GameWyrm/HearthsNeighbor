<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
  <NameField>$HNDevLumineName</NameField>

  <DialogueNode>
    <Name>Start</Name>
    <EntryCondition>DEFAULT</EntryCondition>
    <Dialogue>
      <Page>$HNDevLumineTitle</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>$HNDevPreJamOption</Text>
        <DialogueTarget>LumineJam</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>$HNDevPostJamOption</Text>
        <DialogueTarget>LumineVictory</DialogueTarget>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>LumineJam</Name>
    <Dialogue>
      <Page>$HNDevLumine1</Page>
      <Page>$HNDevLumine2</Page>
      <Page>$HNDevLumine3</Page>
      <Page>$HNDevLumine4</Page>
    </Dialogue>
  </DialogueNode>

  <DialogueNode>
    <Name>LumineVictory</Name>
    <Dialogue>
      <Page>$HNDevLumineV1</Page>
      <Page>$HNDevLumineV2</Page>
      <Page>$HNDevLumineV3</Page>
    </Dialogue>
  </DialogueNode>

</DialogueTree>