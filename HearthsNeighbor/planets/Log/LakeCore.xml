<AstroObjectEntry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/shiplog_schema.xsd">
  <ID>Lake Core</ID>
  <Entry>
    <ID>LAKE_CORE_ROOT</ID>
    <Name>$HNLKName</Name>

    <RumorFact>
      <ID>LK_DESCRIPTION</ID>
      <SourceID>LH_DEAD_LAKE_FACT</SourceID>
      <Text>$HNLKDescription</Text>
    </RumorFact>

    <ExploreFact>
      <ID>LK_RAFT</ID>
      <Text>$HNLKRaft</Text>
    </ExploreFact>

    <Entry>
      <ID>LK_SHRINE_FACT</ID>
      <Name>$HNLKShrineName</Name>

      <RumorFact>
        <ID>LK_SHRINE_RUMOR</ID>
        <Text>$HNLKShrineRumor</Text>
      </RumorFact>

      <ExploreFact>
        <ID>LK_SHRINE_ENTRY</ID>
        <Text>$HNLKShrineEntry</Text>
      </ExploreFact>

      <ExploreFact>
        <ID>LK_SHRINE_STATION</ID>
        <Text>$HNKLKShrineStation</Text>
      </ExploreFact>

      <ExploreFact>
        <ID>LK_SHRINE_LEVER</ID>
        <Text>$HNLKShrineLever</Text>
      </ExploreFact>

      <ExploreFact>
        <ID>LK_SHRINE_MURAL</ID>
        <Text>$HNLKShrineMural</Text>
      </ExploreFact>

      <ExploreFact>
        <ID>LK_SHRINE_POSTER</ID>
        <Text>$HNLKShrinePoster</Text>
      </ExploreFact>
    </Entry>

    <Entry>
      <ID>LK_MAZE_FACT</ID>
      <Name>$HNLKMazeName</Name>

      <RumorFact>
        <ID>LK_MAZE_INFO</ID>
        <Text>$HNLKMazeInfo</Text>
      </RumorFact>

      <ExploreFact>
        <ID>LK_MAZE_GAZEBO</ID>
        <Text>$HNLKMazeGazebo</Text>
      </ExploreFact>
    </Entry>
  </Entry>
  
</AstroObjectEntry>