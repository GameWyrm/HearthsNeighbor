<AstroObjectEntry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/shiplog_schema.xsd">
  <ID>TIMBER_HEARTH</ID>
  <Entry>
    <ID>HN_MUSEUM_PLATE</ID>
    <Name>$HNMuseumPlateName</Name>

    <RumorFact>
      <ID>HN_MUSEUM_PLATE_INTRO</ID>
      <IgnoreMoreToExplore/>
      <Text>$HNMuseumPlateIntro</Text>
      <AltText>
        <Text>$HNMuseumPlateIntroAlt</Text>
        <Condition>HN_MUSEUM_PLATE_DETAILS</Condition>
      </AltText>
    </RumorFact>

    <ExploreFact>
      <ID>HN_MUSEUM_PLATE_DETAILS</ID>
      <IgnoreMoreToExplore/>
      <Text>$HNMuseumPlateDetails</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN_MUSEUM_PLATE_RESOLUTION</ID>
      <IgnoreMoreToExplore/>
      <Text>$HNMuseumPlateResolution</Text>
    </ExploreFact>
  </Entry>

  <Entry>
    <ID>HN_ESCAPE_POD</ID>
    <Name>$HNPodName</Name>
    
    <RumorFact>
      <ID>HN_POD_RUMOR</ID>
      <SourceID>HN_MUSEUM_PLATE</SourceID>
      <Text>$HNPodRumor</Text>
    </RumorFact>

    <ExploreFact>
      <ID>HN_POD_RESOLUTION</ID>
      <Text>$HNPodResolution</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN_POD_THANKS</ID>
      <Text>$HNPodThanks</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>HN_POD_HN2</ID>
      <Text>$HNPodMagistarium</Text>
    </ExploreFact>
    
  </Entry>
</AstroObjectEntry>