<AstroObjectEntry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/shiplog_schema.xsd">

  <ID>Alpine Core</ID>
  <Entry>
    <ID>AC_ROOT</ID>
    <Name>$HNACName</Name>
    <Curiosity>DERELICT_SHIP_ROOT</Curiosity>

    <RumorFact>
      <ID>AC_DESCRIPTION</ID>
      <SourceID>LH_NORTHERN_WASTELAND_FACT</SourceID>
      <Text>$HNACDescription</Text>
    </RumorFact>

    <ExploreFact>
      <ID>AC_RED_HOUSE</ID>
      <Text>$HNACRedHouse</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>AC_RED_MURAL</ID>
      <Text>$HNACRedMural</Text>
    </ExploreFact>
    
    <Entry>
      <ID>AC_CODE_FACT</ID>
      <Name>$HNACCodeName</Name>
      <Curiosity>DERELICT_SHIP_ROOT</Curiosity>

      <ExploreFact>
        <ID>AC_CODE</ID>
        <Text>$HNACCode</Text>
      </ExploreFact>
    </Entry>
  </Entry>

  <Entry>
    <ID>AC_SECRET</ID>
    <Name>$HNACSecret</Name>

    <RumorFact>
      <ID>AC_SECRET_INFO</ID>
      <Text>$HNACSecretInfo</Text>
    </RumorFact>

    <ExploreFact>
      <ID>AC_SECRET_MAP</ID>
      <Text>$HNACSecretMap</Text>
    </ExploreFact>
  </Entry>
  
</AstroObjectEntry>