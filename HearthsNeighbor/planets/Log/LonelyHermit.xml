<AstroObjectEntry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/shiplog_schema.xsd">
  <ID>Lonely Hermit</ID>
  <Entry>
    <ID>LONELY_HERMIT_ROOT</ID>
    <Name>$HNLHName</Name>
    <Curiosity>DERELICT_SHIP_ROOT</Curiosity>

    <RumorFact>
      <ID>LH_ARRIVAL</ID>
      <RumorName>$HNLHRumorName</RumorName>
      <Text>$HNLHArrival</Text>
    </RumorFact>

    <ExploreFact>
      <ID>LH_INFO</ID>
      <Text>$HNLHInfo</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>LH_ELEVATOR</ID>
      <Text>$HNLHElevator</Text>
    </ExploreFact>
  </Entry>

  <Entry>
    <ID>LH_POWER_PLANT_FACT</ID>
    <Name>$HNLHPlantName</Name>

    <RumorFact>
      <ID>LH_PLANT_RUMOR</ID>
