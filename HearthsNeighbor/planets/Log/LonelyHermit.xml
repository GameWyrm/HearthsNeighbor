        <Text>$HNLHVolcanoRumorAlt</Text>
        <Condition>LH_VOLCANO_ELEVATOR</Condition>
      </AltText>
    </RumorFact>

    <ExploreFact>
      <ID>LH_VOLCANO_ELEVATOR</ID>
      <Text>$HNLHVolcanoElevator</Text>
    </ExploreFact>
  </Entry>

  <Entry>
    <ID>LH_DEAD_LAKE_FACT</ID>
    <Name>$HNLHLakeName</Name>
    <Curiosity>DERELICT_SHIP_ROOT</Curiosity>

    <RumorFact>
      <ID>LH_LAKE_RUMOR</ID>
      <SourceID>LONELY_HERMIT_ROOT</SourceID>
      <RumorName>$HNLHLakeRumorName</RumorName>
      <Text>$HNLHLakeRumor</Text>
