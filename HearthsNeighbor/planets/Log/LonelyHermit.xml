      <RumorName>$HNLHPlantRumorName</RumorName>
      <Text>$HNLHPlantRumor</Text>
    </RumorFact>

    <ExploreFact>
      <ID>LH_PLANT_NO_ENTRY</ID>
      <Text>$HNLHPlantNoEntry</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>LH_PLANT_INFO</ID>
      <Text>$HNLHPlantInfo</Text>
    </ExploreFact>
  </Entry>

  <Entry>
    <ID>LH_NORTHERN_WASTELAND_FACT</ID>
    <Name>$HNLHWastelandName</Name>
    <Curiosity>DERELICT_SHIP_ROOT</Curiosity>

    <RumorFact>
      <ID>LH_WASTELAND_RUMOR</ID>
      <SourceID>LONELY_HERMIT_ROOT</SourceID>
      <RumorName>$HNLHWastelandRumorName</RumorName>
      <Text>$HNLHWastelandRumor</Text>
      <AltText>
        <Text>$HNLHWastelandRumorAlt</Text>
        <Condition>LH_WASTELAND_ELEVATOR</Condition>
      </AltText>
    </RumorFact>

    <ExploreFact>
      <ID>LH_WASTELAND_ELEVATOR</ID>
      <Text>$HNLHWastelandElevator</Text>
    </ExploreFact>
  </Entry>

  <Entry>
    <ID>LH_EXTINCT_VOLCANO_FACT</ID>
