<AstroObjectEntry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/shiplog_schema.xsd">
  <ID>Lava Core</ID>
  <Entry>
    <ID>LAVA_CORE_ROOT</ID>
    <Name>$HNLVName</Name>

    <RumorFact>
      <ID>LV_DESCRIPTION</ID>
      <SourceID>LH_EXTINCT_VOLCANO_FACT</SourceID>
      <Text>$HNLVDescription</Text>
    </RumorFact>

    <ExploreFact>
      <ID>LV_BUTTONS</ID>
      <Text>$HNLVButtons</Text>
    </ExploreFact>

    <ExploreFact>
      <ID>LV_SIGNAL</ID>
      <Text>$HNLVSignal</Text>
    </ExploreFact>

    <Entry>
      <ID>LV_FIRST_BUTTON_FACT</ID>
      <Name>$HNLVFirstButtonName</Name>

      <ExploreFact>
        <ID>LV_FIRST_BUTTON_INFO</ID>
        <Text>$HNLVFirstButtonInfo</Text>
      </ExploreFact>
    </Entry>

    <Entry>
      <ID>LV_SECOND_BUTTON_FACT</ID>
      <Name>$HNLVSecondButtonName</Name>

      <ExploreFact>
        <ID>LV_SECOND_BUTTON_INFO</ID>
        <Text>$HNLVSecondButtonInfo</Text>
      </ExploreFact>
    </Entry>

    <Entry>
      <ID>LV_THIRD_BUTTON_FACT</ID>
      <Name>$HNLVThirdButtonName</Name>

      <ExploreFact>
        <ID>LV_THIRD_BUTTON_INFO</ID>
        <Text>$HNLVThirdButtonInfo</Text>
      </ExploreFact>
    </Entry>

    <Entry>
      <ID>LV_SHRINE_FACT</ID>
      <Name>$HNLVShrineName</Name>

      <RumorFact>
        <ID>LV_SHRINE_INFO</ID>
        <RumorName>$HNLVShrineRumorName</RumorName>
        <RumorNamePriority>5</RumorNamePriority>
        <Text>$HNLVShrineInfo</Text>
        <AltText>
          <Text>$HNLVShrineInfoAlt</Text>
          <Condition>LV_SHRINE_ENTRY</Condition>
        </AltText>
      </RumorFact>

      <RumorFact>
        <ID>LV_SHRINE_BUTTON_ONE</ID>
        <SourceID>LV_FIRST_BUTTON_FACT</SourceID>
        <RumorName>$HNLVShrineButtonRumorName</RumorName>
        <RumorNamePriority>0</RumorNamePriority>
        <Text>$HNLVButtonOne</Text>
      </RumorFact>

      <RumorFact>
        <ID>LV_SHRINE_BUTTON_TWO</ID>
        <SourceID>LV_SECOND_BUTTON_FACT</SourceID>
        <RumorName>$HNLVShrineButtonRumorName</RumorName>
        <RumorNamePriority>1</RumorNamePriority>
        <Text>$HNLVButtonTwo</Text>
      </RumorFact>

      <RumorFact>
        <ID>LV_SHRINE_BUTTON_THREE</ID>
        <SourceID>LV_THIRD_BUTTON_FACT</SourceID>
        <RumorName>$HNLVShrineButtonRumorName</RumorName>
        <RumorNamePriority>2</RumorNamePriority>
        <Text>$HNLVButtonThree</Text>
      </RumorFact>

      <ExploreFact>
        <ID>LV_SHRINE_ENTRY</ID>
        <Text>$HNLVShrineEntry</Text>
      </ExploreFact>

      <ExploreFact>
        <ID>LV_SHRINE_MURALS</ID>
        <Text>$HNLVShrineMurals</Text>
      </ExploreFact>
    </Entry>
  </Entry>
  
  
</AstroObjectEntry>